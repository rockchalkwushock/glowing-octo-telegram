		"@testing-library/user-event": "~14.4.3",
		"@types/node": "~18.7.9",
		"@types/react": "~18.0.17",
		"@types/react-dom": "~18.0.6",
		"@typescript-eslint/eslint-plugin": "~5.33.1",
