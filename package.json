		"name": "Cody Brunner",
		"url": "https://codybrunner.com"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "vite build",
		"build-storybook": "build-storybook",
		"clean": "rimraf dist coverage storybook-static",
		"coverage": "vitest run --coverage",
		"dev": "vite",
		"format": "prettier . --write --ignore-unknown",
		"lint": "eslint --cache --fix --ext .ts,.tsx src && stylelint --cache --fix 'src/**/*.css' --ignore-path",
		"preview": "vite preview",
		"storybook": "start-storybook --port 6006",
		"test": "vitest run --passWithNoTests",
		"test:ui": "vitest --ui",
		"test:watch": "vitest dev",
		"type-check": "tsc"
	},
	"dependencies": {
		"react": "~18.2.0",
